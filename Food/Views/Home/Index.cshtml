    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-support-area">
                    <!-- single support -->
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="aa-support-single">
                            <span class="fa fa-truck"></span>
                            <h4>FREE SHIPPING</h4>
                            <P>All shipments are free shipping</P>
                        </div>
                    </div>
                    <!-- single support -->
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="aa-support-single">
                            <span class="fa fa-clock-o"></span>
                            <h4>15 DAYS MONEY BACK</h4>
                            <P>Quick refund</P>
                        </div>
                    </div>
                    <!-- single support -->
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="aa-support-single">
                            <span class="fa fa-phone"></span>
                            <h4>SUPPORT 24/7</h4>
                            <P>Always support users</P>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Support section -->
<!-- Testimonial -->
<section id="aa-testimonial">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-testimonial-area">
                    <ul class="aa-testimonial-slider">
                        <!-- single slide -->
                        <li>
                            <div class="aa-testimonial-single">
                                <img class="aa-testimonial-img" src="images/Users/Avatar.png" alt="testimonial img">
                                <span class="fa fa-quote-left aa-testimonial-quote"></span>
                                <p>I am done with good food. My last 3 meals have all arrived missing the meat. My mom recently passed away so it's really convenient to bring a box over to my father's to make him dinner. At least it would be of they fulfilled the order! I'm sick and tried of having to run out and buy more food. We complain and get a $10 credit but it has now happened 3 times in a row. Some times the food doesn't arrive till the next day. That really helps when you company. Don't waste your time with this company.</p>
                                <div class="aa-testimonial-info">
                                    <p>Allison</p>
                                    <span>Designer</span>
                                    <a href="#">Dribble.com</a>
                                </div>
                            </div>
                        </li>


                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Testimonial -->
<!-- Client Brand -->
<section id="aa-client-brand">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-client-brand-area">
                    <ul class="aa-client-brand-slider">
                        <li><a href="#"><img src="images/client-brand-java.png" alt="java img"></a></li>
                        <li><a href="#"><img src="images/client-brand-jquery.png" alt="jquery img"></a></li>
                        <li><a href="#"><img src="images/client-brand-html5.png" alt="html5 img"></a></li>
                        <li><a href="#"><img src="images/client-brand-css3.png" alt="css3 img"></a></li>
                        <li><a href="#"><img src="images/client-brand-wordpress.png" alt="wordPress img"></a></li>
                        <li><a href="#"><img src="images/client-brand-joomla.png" alt="joomla img"></a></li>
                        <li><a href="#"><img src="images/client-brand-java.png" alt="java img"></a></li>
                        <li><a href="#"><img src="images/client-brand-jquery.png" alt="jquery img"></a></li>
                        <li><a href="#"><img src="images/client-brand-html5.png" alt="html5 img"></a></li>
                        <li><a href="#"><img src="images/client-brand-css3.png" alt="css3 img"></a></li>
                        <li><a href="#"><img src="images/client-brand-wordpress.png" alt="wordPress img"></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Client Brand -->
@{
     void ShowListCategories(string categoryName)
    {
        foreach (var listProductFirst in Model)
    {
        <!-- start single product item -->
        if (listProductFirst.pd_categoryName == categoryName)
        {
            <li>
                <figure>
                    <a class="aa-product-img" href="/productdetail?id=@listProductFirst.pd_Id"><img src="@listProductFirst.pd_Img1" alt="@listProductFirst.pd_Name"></a>
                    <a class="aa-add-card-btn" href="/productdetail?id=@listProductFirst.pd_Id"><span class="fa fa-shopping-cart"></span>Buy</a>
                    <figcaption>
                        <h4 class="aa-product-title"><a href="#">@listProductFirst.pd_Name</a></h4>
                        <span class="aa-product-price">$@listProductFirst.pd_Price</span><span class="aa-product-price"><del>$@listProductFirst.pd_Price</del></span>
                    </figcaption>
                </figure>
                <div class="aa-product-hvr-content">
                </div>

            </li>
        }

    }
    }
    

}